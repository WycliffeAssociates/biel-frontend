{
  "name": "headlesswordpress",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 astro dev",
    "cdev": "wrangler pages dev --proxy=4321 -- pnpm run dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "cfPreview": "wrangler pages dev ./dist",
    "astro": "astro",
    "purgecss": "node ./purge.js",
    "dbcfdeploy": "wrangler pages deployment tail",
    "pagefind": "node ./makePageFindIndex.js",
    "pflocal": "WORDPRESS_GQL_PATH=index.php?graphql SITE_URL=https://bieldev.wpengine.com    TRANSLATIONS_JSON_ENDPOINT=https://bieldev.wpengine.com/wp-content/themes/bb-theme-child/data/translations.json node ./makePageFindIndex.js"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.5",
    "@astrojs/cloudflare": "^9.0.1",
    "@astrojs/sitemap": "^3.0.5",
    "@astrojs/solid-js": "^4.0.1",
    "@solid-primitives/media": "^2.2.6",
    "@wordpress/base-styles": "^4.42.0",
    "@wordpress/block-library": "^8.28.1",
    "astro": "^4.4.0",
    "linkedom": "^0.16.8",
    "solid-js": "^1.8.15",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@unocss/reset": "^0.58.5",
    "@unocss/transformer-directives": "^0.58.5",
    "@unocss/transformer-variant-group": "^0.58.5",
    "glob": "^10.3.10",
    "pagefind": "^1.0.4",
    "purgecss": "^5.0.0",
    "sass": "^1.71.0",
    "unocss": "^0.58.5",
    "vite": "^5.1.3",
    "wrangler": "^3.28.3"
  }
}
